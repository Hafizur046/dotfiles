		)"
		while sleep 5;do
			stat="$(cut -c1 </sys/class/power_supply/BAT0/status)"
			[[ $stat = D ]] || break
			echo "$snd"|base64 -d|lzma -d|pacat
			ntfid=$(
			dbus-send \
				--print-reply=literal --type=method_call \
				--dest=org.freedesktop.Notifications \
				/org/freedesktop/Notifications org.freedesktop.Notifications.Notify \
				string:"i3blocks" uint32:$ntfid string:'dialog-warning' \
				string:'Low Battery' string:'Connect charger.' \
				array:string: dict:string:string: int32:15000 \
				| awk '{print $2}'
			)
		done
	)&
	exit 33
fi
exit 0



